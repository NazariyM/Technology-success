		//begin location
		+b.last-screen.section
			+b.SECTION.location
				+e.label
					+b.section-icon
						+e.pic._location
					+e.title.title-md МЫ РАСПОЛОЖЕНЫ:
				+e.map-wrap
					+e.map#js-home-map
			include partials/_footer
