					+e.IMG(src='img/content/feed_thumbnail.jpg' alt='img').img
					+e.DIV.controls
						+icon('icon-zoom')
				+e.A(href='img/content/feed_img-2.jpg' data-fancybox="feed").pic
					+e.IMG(src='img/content/feed_thumbnail.jpg' alt='img').img
					+e.DIV.controls
						+icon('icon-zoom')
				+e.A(href='img/content/feed_img-2.jpg' data-fancybox="feed").pic
					+e.IMG(src='img/content/feed_thumbnail.jpg' alt='img').img
					+e.DIV.controls
						+icon('icon-zoom')
				+e.A(href='img/content/feed_img-2.jpg' data-fancybox="feed").pic
					+e.IMG(src='img/content/feed_thumbnail.jpg' alt='img').img
					+e.DIV.controls
						+icon('icon-zoom')
	//end feed
	//begin ask
	+b.ask
		+b.section-icon
			+e.pic._question
		+e.container.container
			+e.title.title-md У вас остались вопросы? Звоните прямо сейчас
			+e.in
				+e.A(href='tel:78461234567').phone +7 (846) 123-45-67
				+e.descr или оставьте заявку и мы сами вам перезвоним
			+b.advice.ask__advice
				+e.FORM(action='#!').form
					+e.form-inn
						+e.form-field
							+e.INPUT.input.input(type='text' placeholder='Имя*' name='')
						+e.form-field
							+e.INPUT.input.input(type='tel' placeholder='Телефон*' name='')
						+e.form-field
							+e.BUTTON(type='submit').btn.btn Получить консультацию
	//end ask
	//begin location
	+b.last-screen.section
		+b.SECTION.location
			+e.label
				+b.section-icon
					+e.pic._location
				+e.title.title-md МЫ РАСПОЛОЖЕНЫ:
			+e.map-wrap
				+e.map#js-home-map
	//end location